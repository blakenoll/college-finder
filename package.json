{
  "name": "GQL-Starter",
  "version": "1.0.1",
  "description": "GraphQL boiler plate for express using apollo-server-express",
  "main": "server.js",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "body-parser": "^1.18.3",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.17.5"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "scripts": {
    "start": "npm run build && node ./dist/server.js",
    "dev": "nodemon ./src/server.js --exec babel-node",
    "build": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blakenoll/gql-starter"
  },
  "author": "Blake Noll",
  "license": "MIT",
  "homepage": "https://github.com/blakenoll/gql-starter#readme"
}
